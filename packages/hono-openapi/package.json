        "hono-openapi": "^1.1.0",
        "openapi-types": "^12.1.3"
    },
    "devDependencies": {
        "@jderstd/hono": "workspace:*",
        "hono": "catalog:dev"
    },
    "peerDependencies": {
        "@jderstd/hono": "~0.6.0",
        "@valibot/to-json-schema": "^1.3.0",
        "hono": "catalog:peer",
