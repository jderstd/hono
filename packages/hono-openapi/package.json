{
    "name": "@jderstd/hono-openapi",
    "version": "0.1.0",
    "description": "An OpenAPI helper for Hono",
    "keywords": [
        "jder",
        "hono",
        "openapi",
        "helper",
        "util",
        "utils",
        "ts",
        "typescript",
        "js",
        "javascript"
    ],
    "homepage": "https://github.com/jderstd/hono",
    "bugs": "https://github.com/jderstd/hono/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/jderstd/hono.git",
        "directory": "packages/hono-openapi"
    },
    "license": "MIT",
    "author": {
        "name": "Alpheus",
        "email": "contact@alphe.us"
    },
    "exports": {
